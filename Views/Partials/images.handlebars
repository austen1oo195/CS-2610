<div class="content">
  <div class="squeeze">
    <div class="box-wrap clear">

      {{#each feed}}
        <div class="box">
          <img src="{{this.images.standard_resolution.url}}" kalt = "" />
          <div class="group-btn lk-clear">
              <button class="btn accent-1 pull--left">Likes: {{this.likes.count}}</button>
              <button class="btn accent-1 pull--right">Comments: {{this.comments.count}}</button>
          </div>
        </div>
      {{/each}}

    </div>
  </div>
</div>
